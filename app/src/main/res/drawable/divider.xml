<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:end="@dimen/divider_spacing"
        android:gravity="fill_vertical"
        android:left="@dimen/divider_spacing"
        android:right="@dimen/divider_spacing"
        android:start="@dimen/divider_spacing">
        <shape android:shape="rectangle">
            <size
                android:width="100dp"
                android:height="1dp" />

            <solid android:color="@color/backgroundColor" />
        </shape>
    </item>
    <item
        android:end="@dimen/divider_spacing"
        android:gravity="fill_vertical"
        android:left="@dimen/divider_spacing"
        android:right="@dimen/divider_spacing"
        android:start="@dimen/divider_spacing">
        <shape android:shape="rectangle">
            <size
                android:width="100dp"
                android:height="1dp" />

            <solid android:color="@color/dividerColor" />
        </shape>
    </item>
</layer-list>
